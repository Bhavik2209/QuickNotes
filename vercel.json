{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"  // Ensure this matches your build output directory
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",  // Match all routes
      "dest": "/index.html"  // Redirect all requests to index.html
    }
  ]
}